<h1>Login</h1>
<%= form_for :article, url: articles_path do |f| %>
 

  <p>
    <%= f.label :email %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :text %>
  </p>
 
  <p>
    <%= f.submit "Login" %>
  </p>

<%= link_to 'signup', articles_path %>
<% end %>