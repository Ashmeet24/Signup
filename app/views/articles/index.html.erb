<h1>SignUp Page</h1>
<%= form_for :article   do |f| %>

<p>
  <%= f.label :name %>
  <%= f.text_field :name %> <br />
</p>
<p>
  <%= f.label :email %>
  <%= f.text_field :email%> <br />
</p>
<p>
  <%= f.label :password %>
  <%= f.password_field :password %> <br />
</p>
<p>
  <%= f.label :confirm_password %>
  <%= f.password_field :confirm_password %> <br />
</p>
<p>
  <%= f.submit "Login account" %>
</p>
<% end %>

